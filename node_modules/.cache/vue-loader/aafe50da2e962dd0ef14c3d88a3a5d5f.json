rm\" year-format=\"{value}年\"\n      month-format=\"{value}月\" date-format=\"{value}日\" @visible-change=\"visbleChange\"></mt-datetime-picker>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"EllDatePicker\",\n  props: {\n    form: {\n      type: Object,\n      default: () => {\n        return {};\n      }\n    },\n    post: {\n      type: Number\n    },\n    name: {\n      type: String\n    },\n    disabled: {\n      type: Boolean,\n      default: false\n    }\n  },\n  filters: {\n    getFormatDate: function(value) {\n      if (!value) return \"\";\n      var seperator1 = \"-\";\n      var year = value.getFullYear();\n      var month = value.getMonth() + 1;\n      var strDate = value.getDate();\n      if (month >= 1 && month <= 9) {\n        month = \"0\" + month;\n      }\n      if (strDate >= 0 && strDate <= 9) {\n        strDate = \"0\" + strDate;\n      }\n      var currentdate = year + seperator1 + month + seperator1 + strDate;\n      return currentdate;\n    }\n  },\n  methods: {\n    visbleChange(val) {\n      if (val) {\n        this.closeTouch();\n      } else {\n        this.openTouch();\n      }\n    },\n    /*解决页面层级相互影响滑动的问题*/\n    closeTouch() {\n      document\n        .getElementsByTagName(\"body\")[0]\n        .addEventListener(\"touchmove\", this.handler, { passive: false }); //阻止默认事件\n    },\n    openTouch() {\n      document\n        .getElementsByTagName(\"body\")[0]\n        .removeEventListener(\"touchmove\", this.handler, { passive: false }); //打开默认事件\n    },\n    openPicker() {\n      this.$refs.picker.open();\n    },\n    confirm(v) {\n      this.form[this.post] = v;\n    }\n  }\n};\n</script>\n<style lang='less' scoped>\n.date-box {\n  width: 100%;\n  padding: 0 10px 10px;\n  p {\n    background: #f1f1f1;\n    border-radius: 4px;\n    height: 40px;\n    line-height: 40px;\n    padding: 0 10px;\n    font-size: 12px;\n  }\n}\n</style>"]}]}